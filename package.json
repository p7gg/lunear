		"postcss": "^8.4.41",
		"remix-flat-routes": "^0.6.5",
		"tailwindcss": "^3.4.10",
		"type-fest": "^4.26.0",
		"typescript": "^5.5.4",
		"vite": "^5.4.2",
		"vite-tsconfig-paths": "^5.0.1"
	},
	"engines": {
		"node": ">=22.0.0"
	}
}
