{
	"name": "lunear",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "bunx --bun remix vite:build",
		"dev": "bun run ./server/index.ts",
		"start": "NODE_ENV=production bun run --bun ./server/index.ts",
		"typecheck": "bunx --bun tsc",
		"update-deps": "bunx --bun taze -I",
		"migrations:generate": "drizzle-kit generate",
		"migrations:run": "drizzle-kit migrate",
		"migrations:push": "drizzle-kit push",
		"fmt:check": "biome format .",
		"fmt:fix": "biome format --write .",
		"lint:check": "biome lint .",
		"lint:fix": "biome lint --write .",
		"lint:unsafe": "biome check --unsafe .",
		"check": "biome check .",
		"fix": "biome check --write .",
		"unsafe": "biome check --write --unsafe ."
	},
	"dependencies": {
		"@conform-to/react": "^1.1.5",
		"@conform-to/zod": "^1.1.5",
		"@epic-web/client-hints": "^1.3.3",
		"@epic-web/invariant": "^1.0.0",
		"@hookform/resolvers": "^3.9.0",
		"@libsql/client": "^0.10.0",
		"@lucia-auth/adapter-drizzle": "^1.1.0",
		"@paralleldrive/cuid2": "^2.2.2",
		"@radix-ui/react-avatar": "^1.1.0",
		"@radix-ui/react-checkbox": "^1.1.1",
		"@radix-ui/react-dialog": "^1.1.1",
		"@radix-ui/react-dropdown-menu": "^2.1.1",
		"@radix-ui/react-label": "^2.1.0",
		"@radix-ui/react-popover": "^1.1.1",
		"@radix-ui/react-radio-group": "^1.2.0",
		"@radix-ui/react-scroll-area": "^1.1.0",
		"@radix-ui/react-select": "^2.1.1",
		"@radix-ui/react-slot": "^1.1.0",
		"@radix-ui/react-switch": "^1.1.0",
		"@radix-ui/react-tabs": "^1.1.0",
		"@radix-ui/react-toast": "^1.2.1",
		"@radix-ui/react-tooltip": "^1.1.2",
		"@radix-ui/react-visually-hidden": "^1.1.0",
		"@remix-run/express": "^2.11.2",
		"@remix-run/node": "^2.11.2",
		"@remix-run/react": "^2.11.2",
		"clsx": "^2.1.1",
		"cmdk": "1.0.0",
		"compression": "^1.7.4",
		"cva": "^1.0.0-beta.1",
		"drizzle-orm": "^0.33.0",
		"express": "^4.19.2",
		"isbot": "^5.1.17",
		"lucia": "^3.2.0",
		"morgan": "^1.10.0",
		"oslo": "^1.2.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.53.0",
		"remix-toast": "^1.2.1",
		"remix-utils": "^7.6.0",
		"tailwind-merge": "^2.5.2",
		"tailwindcss-animate": "^1.0.7",
		"vaul": "^0.9.1",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@remix-run/dev": "^2.11.2",
